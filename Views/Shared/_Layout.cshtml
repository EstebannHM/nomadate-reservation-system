<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - nomadate_web</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/nomadate_web.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        
    </header>
    <div class="container">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer>
        <div class="container">
            &copy; 2025 - nomadate_web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    @await Html.PartialAsync("_AuthModal")

    <script src="~/js/auth.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>
</body>
</html>
